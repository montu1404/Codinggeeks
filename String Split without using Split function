///// Program to split a string without using split method in Java  (easiest way)

import java.util.*;
import java.lang.*;
import java.io.*;
 
/* Name of the class has to be "Main" only if the class is public. */
class StrwoSplit
{
	public static void main (String[] args) throws java.lang.Exception
	{
		String s1= "hi i am mohit";
		int i;
		int n= s1.length();
 
		for(i=0; i<n; i++){
			if(s1.charAt(i)== ' ')
			{
				System.out.println();  // the delimiter is Enter that is next line choose your delimiter accordingly
			}
			else
			System.out.print(s1.charAt(i));
		}
 
	}
}
